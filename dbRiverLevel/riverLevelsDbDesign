CREATE TABLE "albums" ( 
[AlbumId] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, 
[Title] NVARCHAR(160) NOT NULL, 
[ArtistId] INTEGER NOT NULL, 
FOREIGN KEY ([ArtistId]) 
REFERENCES "artists" ([ArtistId]) 
ON DELETE NO ACTION 
ON UPDATE NO ACTION )

see Development/sqliteTest/Sample DB



CREATE TABLE "rivers" ( 
[RiverId] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, 
[siteName] NVARCHAR(256) NOT NULL, 
[zoneAbbreviation] NVARCHAR(256) NOT NULL,
[unitCode] NVARCHAR(32) NOT NULL )

CREATE TABLE "levels" ( 
[LevelId] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, 
[levelValue] NVARCHAR(32) NOT NULL, 
[dateTime] NVARCHAR(128) NOT NULL,
[riverId] INTEGER NOT NULL, 
FOREIGN KEY ([riverId])
REFERENCES "rivers" ([RiverId])
ON DELETE NO ACTION
ON UPDATE NO ACTION )


